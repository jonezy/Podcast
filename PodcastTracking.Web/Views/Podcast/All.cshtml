@model System.Collections.Generic.List<PodcastTracking.Web.Models.PodcastViewModel>
@{
    ViewBag.Title = "All Podcasts";
}

<h2>All Podcasts</h2>

<div class="row">
@foreach (var podcast in Model)
{
    <div class="col-lg-12">
        @Html.ActionLink(@podcast.Title, "index", new { id = @podcast.PodcastId })
    </div>
}
</div>